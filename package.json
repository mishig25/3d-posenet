{
  "name": "3d-posenet",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow-models/posenet": "0.1.2",
    "@tensorflow/tfjs": "0.11.4",
    "babylonjs": "^3.2.0",
    "markdown": "^0.5.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-loading": "^2.0.3"
  },
  "scripts": {
    "watch": "NODE_ENV=development BPATH=dist parcel --no-hmr --open index.html ",
